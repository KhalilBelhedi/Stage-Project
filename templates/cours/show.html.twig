

{% block title %}Cours{% endblock %}

{% block body %}
    <h1>Cours</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cour.id }}</td>
            </tr>
            <tr>
                <th>CoursBH_achat</th>
                <td>{{ cour.coursBHAchat }}</td>
            </tr>
            <tr>
                <th>CoursBH_vente</th>
                <td>{{ cour.coursBHVente }}</td>
            </tr>
            <tr>
                <th>CoursMoy</th>
                <td>{{ cour.coursMoy }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ cour.date ? cour.date|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cours_index') }}">back to list</a>

    <a href="{{ path('app_cours_edit', {'id': cour.id}) }}">edit</a>

    {{ include('cours/_delete_form.html.twig') }}
{% endblock %}
